# Example .env file after setting up Discord webhook

DATABASE_URL=postgresql://postgres:123456@localhost:5432/blog
NODE_ENV=development

# Discord Error Notifications Configuration
# Replace the URL below with your actual Discord webhook URL
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/1234567890123456789/abcdefghijklmnopqrstuvwxyz-1234567890_ABCDEFGHIJKLMNOPQRSTUVWXYZ
DISCORD_NOTIFICATIONS_ENABLED=true

# Your actual .env file should have a real webhook URL from Discord
# The URL format is: https://discord.com/api/webhooks/[WEBHOOK_ID]/[WEBHOOK_TOKEN]
